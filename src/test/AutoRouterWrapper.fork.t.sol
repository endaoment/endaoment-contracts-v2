        deal(usdc, address(this), 1e10 ether);
        deal(usdt, address(this), 1e10 ether);
        deal(weth, address(this), 1e10 ether);
    }

    function swap(address _tokenIn, address _tokenOut, address _recipient, uint256 _amount, bytes memory _data)
        public
    {
        uint256 prevBalance = _tokenOut == eth ? _recipient.balance : ERC20(_tokenOut).balanceOf(_recipient);
        // Warping block.timestamp because https://github.com/Uniswap/smart-order-router CLI sample data outputs have 100 deadline.
        // https://github.com/Uniswap/swap-router-contracts/blob/c696aada49b33c8e764e6f0bd0a0a56bd8aa455f/contracts/base/MulticallExtended.sol#L18
        // https://github.com/Uniswap/v3-periphery/blob/5bcdd9f67f9394f3159dad80d0dd01d37ca08c66/contracts/base/PeripheryValidation.sol#L8
        vm.warp(100);
        if (_tokenIn != eth) {
            wrapper.swap(
                _tokenIn,
                _tokenOut,
                _recipient,
                _amount, // Match this value with the amount provided to the auto router.
                _data
            );
        } else {
            wrapper.swap{value: _amount}(
                _tokenIn,
                _tokenOut,
                _recipient,
                _amount, // Match this value with the amount provided to the auto router.
                _data
            );
        }

        uint256 currentBalance = _tokenOut == eth ? _recipient.balance : ERC20(_tokenOut).balanceOf(_recipient);

        assertGt(currentBalance, prevBalance);
    }

